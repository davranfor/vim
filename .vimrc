syntax on

" Disable matching parenthesis highlighting
let g:loaded_matchparen=1

" Colors at /usr/share/vim/vim90/colors
colorscheme desert
highlight LineNr ctermfg=grey

set number
set ruler
set showmode
set ignorecase
set belloff=all
set tags=../tags

set expandtab
set tabstop=4
set shiftwidth=4
